<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="Content-Language" content="pt-br">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>"@ViewBag.Title"</title>
    <meta name="keywords" content="@ViewBag.Content" />
    <meta name="author" CONTENT="Di Fiore Soluções Corporativas, https://www.difiore.com.br">
    <meta name="description" content="No MINI voc&ecirc; encontra receitas f&aacute;ceis e saud&aacute;veis que uso para orientar meus pacientes do consult&oacute;rio, al&eacute;m de como eu trabalho, cursos e muito mais!" />
    <meta id="currency" property="BRL" />
    <meta name="google-site-verification" content="mIl3mU_fvU-pEAVIugg_jEPyhbOU4APl5C-OGkS3Fvs" />
    <link href="~/Content/Site.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Início", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Golden Milk", "GoldenMilk", "Dicas")</li>
                    <li>@Html.ActionLink("Receitas", "Mais_receitas", "Receitas")</li>
                    <li>@Html.ActionLink("Cursos e eventos", "Index", "Cursos")</li>
                    <li>@Html.ActionLink("Biblioteca", "Index", "Dicas")</li>
                    <li class="dropdown">
                        <a class="dropdown-toggle" data-toggle="dropdown" href="@Url.Action("Atendimento", "Home")">Atendimento <span class="caret"></span></a>
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("Atendimento", "Atendimento", "Home")</li>
                            <li>@Html.ActionLink("Fitoterapia", "Fitoterapia", "Home")</li>
                            <li>@Html.ActionLink("Serviços", "Servicos", "Home")</li>
                            <li>@Html.ActionLink("Curriculo Lates", "Curriculo_Lattes", "Home")</li>
                        </ul>
                    </li>
                    <li class="dropdown">
                        <a class="dropdown-toggle" data-toggle="dropdown" href="@Url.Action("Contact", "Contact")">Contato <span class="caret"></span></a>
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("Fale comigo", "Contact", "Contact")</li>
                            <li>@Html.ActionLink("Localização", "Mapa", "Contact")</li>
                        </ul>
                    </li>
                    <li>@Html.ActionLink("Download", "Index", "File")</li>
                </ul>
            </div>
        </div>
    </div>
    <div class="container">
        <div class="row">
            <div div class="hidden-md hidden-lg">
                <br />
                <br />
            </div>
        </div>
    </div>
    <div class="container body-content">
        <script>
            (function () {
                var cx = '010159411092776514353:dzk-djlflo4';
                var gcse = document.createElement('script');
                gcse.type = 'text/javascript';
                gcse.async = true;
                gcse.src = 'https://cse.google.com/cse.js?cx=' + cx;
                var s = document.getElementsByTagName('script')[0];
                s.parentNode.insertBefore(gcse, s);
            })();
        </script>
        <gcse:search></gcse:search>
        @RenderBody()
        <hr />
        <footer>
            <div class="container">
                <div class="row">
                    <div class="col-sm-6 col-md-6 col-md-6 col-lg-6">
                        <p style="font-size: small" class="text-left">&copy; 2018 - @DateTime.Now.Year Manuela Ibi Nutrição Integrada. Todos direitos reservados</p>
                    </div>
                    <div class="col-sm-6 col-md-6 col-md-6 col-lg-6">
                        <p style="font-size: small" class="text-right">Desenvolvido por: <a href="https://www.difiore.com.br/" target="_blank">Di Fiore Soluções Corporativas</a></p>
                    </div>
                </div>
            </div>
        </footer>
    </div>
</body>
</html>
